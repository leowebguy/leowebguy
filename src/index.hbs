<!doctype html>
<html lang="en" data-bs-theme="dark">
{{> partials/head
    title='leowebguy'
    description='nom nom nom nom nom nom'
    canonical='https://leowebguy.com/' }}
<body class="d-flex flex-column min-vh-100">
    <div class="container my-6">
        <div class="p-4 border shadow-lg">{{!-- rounded --}}
            <div class="row align-items-center">
                <div class="col-lg-4 offset-lg-1 mb-4 mb-lg-0 order-lg-2">
                    <img src="/img/leo.jpg" alt="leowebguy" class="img-fluid">{{!-- rounded --}}
                </div>
                <div class="col-lg-7 order-lg-1 ps-xl-5">
                    {{> partials/body }}
                    <div class="d-block my-5"></div>
                    <div class="d-flex flex-row gap-2">
                        <a href="//github.com/leowebguy" target="_blank" class="btn btn-outline-primary"><i
                            class="bi bi-github"></i></a>
                        <a href="//linkedin.com/in/leowebguy" target="_blank"
                           class="btn btn-outline-primary"><i class="bi bi-linkedin"></i></a>
                        <a href="//stackoverflow.com/users/3058927/leo-leoncio" target="_blank"
                           class="btn btn-outline-primary"><i class="bi bi-stack-overflow"></i></a>
                        <a href="javascript:void(0)" type="button" class="btn btn-secondary contact"><i
                            class="bi bi-envelope-at"></i></a>{{!-- style="font-size: 1.5rem;" --}}
                    </div>
                </div>
            </div>
        </div>
    </div>
    {{> partials/modal id='contact' }}
    <script src="{{{ mixJs }}}"></script>
</body>
</html>
